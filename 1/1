#include <iostream>
using namespace std;
int main(){
int num = 5;
int n;
cin»n;
int sum,min;
int *a=new int [num-1];
for (int i=0;i<num; i++){
cin»a[i%num];
}
min=1;
int x;
for (int i=num;i<n;i++){
cin»x;
if (a[i%num]<min) min=a[i%num];
if (x+min<sum) sum=x+min;
a[i%num]= x;
}
cout«endl«sum;
return 0;
}
